{
    "name": "salmanzafar/laravel-inlfux",
    "description": "A Package to connect laravel to influxdb",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Salman Zafar",
            "email": "salmanzafar949@gmail.com"
        }
    ],
    "keywords": [
        "laravel",
        "laravel5",
        "influx-laravel",
        "laravel-influx",
        "influxdb"
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "laravel/framework": ">=5.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Salman\\Influx\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Salman\\Influx\\InfluxServiceProvider"
            ],
        }
    }
}
